<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>

    <IsPackable>false</IsPackable>

    <AssemblyName>WebEID.Security.Tests</AssemblyName>

    <RootNamespace>WebEID.Security.Tests</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Validator\AuthToken.txt" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Validator\AuthToken.txt" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="coverlet.collector" Version="3.0.3">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="nunit" Version="3.12.0" />
    <PackageReference Include="NUnit3TestAdapter" Version="3.15.1" />
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="16.4.0" />
    <PackageReference Include="System.Runtime.Caching" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\WebEid.Security\WebEid.Security.csproj" />
  </ItemGroup>

</Project>
